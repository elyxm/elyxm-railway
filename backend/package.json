{
  "name": "medusa-2.0-boilerplate-backend",
  "version": "0.0.2",
  "description": "A Prebaked Medusa 2.0 backend",
  "author": "Rasmus Puls",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && node src/scripts/postBuild.js",
    "db:seed": "medusa exec ./src/scripts/seed.ts",
    "db:reset": "pnpm postgres:reset && pnpm ib && pnpm db:seed && pnpm create:admin",
    "db:migrate": "medusa db:migrate",
    "db:generate:delivery": "medusa db:generate delivery",
    "db:generate:restaurant": "medusa db:generate restaurant",
    "db:sync-links": "npx medusa db:sync-links",
    "ib": "init-backend",
    "start": "init-backend && cd .medusa/server && medusa start --verbose",
    "dev": "medusa develop",
    "email:dev": "email dev --dir=./src/modules/email-notifications/templates --port=3002",
    "create:admin": "medusa user --email admin@elyxm.local --password password"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.8.8",
    "@medusajs/cli": "2.8.8",
    "@medusajs/core-flows": "2.8.8",
    "@medusajs/dashboard": "2.8.8",
    "@medusajs/framework": "2.8.8",
    "@medusajs/icons": "latest",
    "@medusajs/medusa": "2.8.8",
    "@medusajs/notification-sendgrid": "2.8.8",
    "@medusajs/payment-stripe": "2.8.8",
    "@medusajs/types": "2.8.8",
    "@medusajs/ui": "latest",
    "@medusajs/utils": "2.8.8",
    "@medusajs/workflow-engine-redis": "2.8.8",
    "@medusajs/workflows-sdk": "2.8.8",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@react-email/components": "^0.0.26",
    "@rokmohar/medusa-plugin-meilisearch": "^1.1.4",
    "awilix": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "16.3.1",
    "express": "^4.17.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.379.0",
    "medusajs-launch-utils": "^0.0.16",
    "minio": "^8.0.3",
    "pg": "^8.13.1",
    "prism-react-renderer": "^2.0.4",
    "resend": "4.0.1",
    "ulid": "^2.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.8.8",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.37",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mime": "1.3.5",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "^3.0.4",
    "react-router-dom": "6.20.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  }
}
