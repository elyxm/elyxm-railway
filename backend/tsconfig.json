{
  "compilerOptions": {
    "target": "es2019",
    "allowJs": true,
    "esModuleInterop": true,
    "module": "node16",
    "moduleResolution": "node16",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "strict": true,
    "skipDefaultLibCheck": true,
    "declaration": true,
    "sourceMap": false,
    "outDir": "./.medusa/server",
    "rootDir": "./",
    "baseUrl": ".",
    "paths": {
      "*": ["./src/*"],
      "@links/*": ["./src/links/*"],
      "@modules/*": ["./src/modules/*"],
      "@subscribers/*": ["./src/subscribers/*"],
      "@utils/*": ["./src/utils/*"],
      "@workflows/*": ["./src/workflows/*"]
    },
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "checkJs": false
    // Adding another comment to force TS server restart
  },
  "ts-node": {
    "swc": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
